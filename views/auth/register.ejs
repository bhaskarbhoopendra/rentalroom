<% include ../partials/header %>

<div class="container form">
  <div class="sec" style="width:50%;margin:25px auto">
    <h1 class="text-center text-muted">SIGN UP..</h1>
    <form class="form-group" action="/auth/register" method="post">
      <input class="form-control" type="text" name="username" placeholder="Username" required>

      <input class="form-control" type="password" name="password" placeholder="Password" required>

      <input class="form-control" type="text" name="firstName" placeholder="firstName" required>

      <input class="form-control" type="text" name="lastName" placeholder="lastname" required>

      <input class="form-control" type="text" name="Phone No" placeholder="image" required>

      <input class="form-control" type="text" name="interest" placeholder="location" required>


      <input class="form-control" type="text" name="college" placeholder="city " required>

      <input class="form-control" type="text" name="year" placeholder="year" required>

      <input class="form-control" type="text" name="age" placeholder="zone " required>

      <input class="form-control" type="text" name="gender" placeholder="gender" required>

      <input class="form-control" type="email" name="fatherName" placeholder="Occupation" required>
      <input class="form-control" type="email" name="fatherName" placeholder="Occupation detail" required>
      <button class="form-control btn btn-md btn-success">Sign up</button>
    </form>
  </div>
</div>

<% include ../partials/footer %>